{
  "name": "swagger-elm",
  "version": "0.0.0",
  "description": "",
  "main": "dist/elm.js",
  "scripts": {
    "test-prepare": "cat tests/fixtures/swagger.json | swagger-to-elm > tests/Decoder.elm",
    "test-compile": "cd tests && elm-make --warn Integration.elm --output integration.js",
    "test-integration": "npm run test-prepare -s && npm run test-compile -s && cat tests/fixtures/data.json | elm-stdio --name=Integration tests/integration.js",
    "test": "elm-test && npm run test-integration -s",
    "build": "elm-make --warn source/Generate.elm  --output dist/elm.js",
    "dev": "nodemon -w source -w tests/Tests.elm -e 'elm' -x \"npm run build && npm run test -s\"",
    "debug": "nodemon -w dist -x \"cat example/swagger.json | swagger-to-elm\""
  },
  "bin": {
    "swagger-to-elm": "./bin/swagger-to-elm"
  },
  "author": "Andreas Hultgren",
  "license": "ISC",
  "dependencies": {
    "node-elm-stdio": "github:ahultgren/node-elm-stdio"
  },
  "devDependencies": {
    "elm-test": "^0.17.3"
  }
}
